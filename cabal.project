packages: .

package *
    optimization: 2
    ghc-options:
        -split-sections
        -funbox-strict-fields

package impli-web
    ghc-options:
        -split-sections
        -funbox-strict-fields
        -fforce-recomp
    with-compiler: javascript-unknown-ghjs-ghc
    with-hc-pkg: javascript-unknown-ghcjs-ghc-pkg
